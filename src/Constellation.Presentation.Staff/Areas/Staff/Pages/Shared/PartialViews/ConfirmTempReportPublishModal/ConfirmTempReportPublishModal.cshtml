@model Constellation.Presentation.Staff.Areas.Staff.Pages.Shared.PartialViews.ConfirmTempReportPublishModal.ConfirmTempReportPublishModalViewModel

<div class="modal-header">
    <h5 class="modal-title">Publish Report</h5>
</div>
<div class="modal-body">
    <div class="row">
        <div class="form-group col">
            <span class="control-label">Are you sure you wish to publish the External Report with file name @Model.FileName? This report will not be included in a mail merge if it has been published.</span>
        </div>
    </div>
</div>
<div class="modal-footer">
    <a asp-page-handler="PublishReport" asp-route-reportId="@Model.ReportId" class="btn btn-success show-loader">Publish Report</a>
    <button type="button" class="btn btn-secondary" data-dismiss="modal" data-bs-dismiss="modal">Close</button>
</div>